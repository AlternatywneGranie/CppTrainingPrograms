//Create a vector of real numbers, initialize it with 5 values, and display them in reverse order.

#include <iostream>
#include <vector>
#include <algorithm>

int main() {
	std::vector<int> backward = { 1,2,3,4,5 };
	std::sort(backward.rbegin(), backward.rend());
	std::cout << "Elementy wektora: ";
	for (const auto& elem : backward) {
		std::cout << elem << " ";
	}
	std::cout << std::endl;
	return 0;
}
